<h1><%= user.name %></h1>

<h3>Review history:</h3>


<% if(user.reviews) { %>
    <% user.reviews.forEach(function(a) { %>
      <div class="well">
        <h3>
          <a href="/products/<%= a.id %>"><%= a.content %></a>
        </h3>
        
      </div>
    <% }) %>
  <% } else { %>
    <h3>No reviews yet!</h3>
  <% } %>

